#include <iostream>

using namespace std;

static const int CHILD_NUM = 26;

struct TrieNode {
  struct TrieNode* child[CHILD_NUM];
  bool end;
};

// int main()
// {
//   struct TrieNode *trie;
//   cout << sizeof(trie->child) << endl;
//   cout << sizeof(TrieNode::child) << endl;
//   return 0;
// }

bool Find(const string &str, struct TrieNode *trie)
{
  int strSize = str.size();
  bool result = true;
  
  for (int i = 0; i < strSize; i++) {
    if (trie->child[str[i]-'a'] == NULL) {
      struct TrieNode *temp = new struct TrieNode;
      temp->end = false;
      memset(temp->child, 0, sizeof(temp->child));
      trie->child[str[i]-'a'] = temp;
      result = false;
    } else {
    
    }
    trie = trie->child[str[i]-'a'];
  }
  
  return result;
}

